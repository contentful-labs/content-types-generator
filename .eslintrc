{
  "extends": [
    "oclif",
    "oclif-typescript"
  ],
  "rules": {
    "operator-linebreak": [
      "error",
      "after",
      {
        "overrides": {
          "?": "before",
          ":": "before"
        }
      }
    ],
    "semi": [
      "error",
      "always"
    ],
    "indent": [
      "error",
      4
    ]
  },
  "overrides": [
    {
      "files": [
        "src/renderer/type/base-content-type-renderer.ts"
      ],
      "rules": {
        "@typescript-eslint/no-unused-vars": [0]
      }
    }
  ]
}
